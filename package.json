    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "serverless": "^1.34.1",
    "serverless-plugin-optimize": "^4.0.0-rc.1",
    "serverless-plugin-typescript": "^1.1.5",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-fishbrain": "^4.1.0",
    "typescript": "^3.2.1"
