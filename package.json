{
  "name": "cloudwatch-sidekiq-stats",
  "version": "1.0.0",
  "description": "Lambda function to fetch Sidekiq stats and put them in CloudWatch",
  "main": "index.ts",
  "dependencies": {
    "aws-sdk": "^2.370.0",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.15",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
